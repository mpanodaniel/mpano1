import java.awt.*;
import java.applet.*;

public class FirstApplet1 extends Applet{
public void init (){
setBackground(Color.blue);
setForeground(Color.white);
}
public void paint(Graphics g){
g.drawString("MULTIPLICATION",23,45);
g.drawLine(30,110,190,190);
g.drawLine(35,200,150,100);
g.drawLine(100,70,100,200);
g.drawLine(20,145,200,145);
showStatus("@multiplicationlines");
}
}
